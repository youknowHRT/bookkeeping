tDay; i++) {\r\n      //遍历每天的总支出\r\n      dayOfMonthX[i] = i + 1\r\n\r\n      let eachDayCost = expenseList\r\n        .filter((item) => {\r\n          return parseInt(dayjs(item.createAt).format('DD')) === i+1\r\n        })\r\n        .map((item) => parseInt(item.amount))\r\n        .reduce((i, j) => {\r\n          return i + j\r\n        }, 0)\r\n      expenseArr[i] = eachDayCost === 0 ? '' : eachDayCost\r\n\r\n      let eachDayIncome = incomeList\r\n        .filter((item) => {\r\n          return parseInt(dayjs(item.createAt).format('DD')) === i+1\r\n        })\r\n        .map((item) => parseInt(item.amount))\r\n        .reduce((i, j) => {\r\n          return i + j\r\n        }, 0)\r\n      incomeArr[i] = eachDayIncome === 0 ? '' : eachDayIncome\r\n    }\r\n    return { expenseArr, incomeArr, dayOfMonthX }\r\n  }\r\n\r\n  const eachTagCost = (e) => {\r\n    //这块数据是用于甜甜圈图表的\r\n    //获取所有的tag\r\n    let accountList = e === '-' ? expenseList : incomeList\r\n    const tagCost = []\r\n    let value = 0\r\n    let name = ''\r\n    let allExpenseTags = accountList.map((item) => {\r\n      return item.tag.value\r\n    })\r\n    const pureTagList = [...new Set(allExpenseTags)] //allExpenseTags就是我们获取的tag集合并且已经去重\r\n    for (let i = 0; i < pureTagList.length; i++) {\r\n      let arr = accountList.filter((item) => {\r\n        return item.tag.value === pureTagList[i]\r\n      })\r\n      value = arr.reduce((x, y) => x + parseInt(y.amount), 0)\r\n      name = pureTagList[i]\r\n      tagCost.push({ value, name })\r\n    }\r\n\r\n    return { tagCost, pureTagList }\r\n  }\r\n  const [controller, setController] = useState(false)\r\n  function controllerHandle(moneyType) {\r\n    //用于bar和pie图表支出和收入的数据联动\r\n    moneyType === '+' ? setController(true) : setController(false)\r\n  }\r\n\r\n  return (\r\n    <div>\r\n      <h1>{/* 我是图表页面{filterLocalList} */}</h1>\r\n      <div>\r\n        <span>本月总支出{monthLastDay}</span>\r\n        <BarChart value={arrAccount()} controller={controllerHandle} />\r\n      </div>\r\n      <div>\r\n        <span>本月分类占比</span>\r\n        <div>图表</div>\r\n        {controller === false ? (\r\n          <Doughnut pureTagList={eachTagCost('-')} tagCost={eachTagCost('-')} />\r\n        ) : (\r\n          <Doughnut pureTagList={eachTagCost('+')} tagCost={eachTagCost('+')} />\r\n        )}\r\n      </div>\r\n    </div>\r\n  )\r\n}\r\n","E:\\Vue-Projectttttttttttttt\\React-project\\bookkeeping\\src\\pages\\general.js",["88"],"E:\\Vue-Projectttttttttttttt\\React-project\\bookkeeping\\src\\pages\\404.js",[],"E:\\Vue-Projectttttttttttttt\\React-project\\bookkeeping\\src\\components\\nav.js",[],"E:\\Vue-Projectttttttttttttt\\React-project\\bookkeeping\\src\\components\\numberPad.js",["89","90"],"E:\\Vue-Projectttttttttttttt\\React-project\\bookkeeping\\src\\components\\tagList.js",[],"E:\\Vue-Projectttttttttttttt\\React-project\\bookkeeping\\src\\store\\datahelper.js",[],"E:\\Vue-Projectttttttttttttt\\React-project\\bookkeeping\\src\\store\\iconList.js",[],"E:\\Vue-Projectttttttttttttt\\React-project\\bookkeeping\\src\\components\\icon.js",[],"E:\\Vue-Projectttttttttttttt\\React-project\\bookkeeping\\src\\assets\\icon\\iconfont.js",["91","92","93","94","95","96","97","98","99","100","101","102"],"E:\\Vue-Projectttttttttttttt\\React-project\\bookkeeping\\src\\pages\\totalLabel.js",[],"E:\\Vue-Projectttttttttttttt\\React-project\\bookkeeping\\src\\store\\dateFormatter.js",[],"E:\\Vue-Projectttttttttttttt\\React-project\\bookkeeping\\src\\components\\charts\\barChart.js",["103"],"import echarts from 'echarts'\r\nimport React, { useEffect } from 'react'\r\nimport styled from 'styled-components'\r\n\r\nconst width = document.documentElement.clientWidth\r\nconst height = document.documentElement.clientWidth * 0.8\r\nconst BarchartWrap = styled.div``\r\n\r\nexport default function BarChart(props) {\r\n  // const { expenseArr, incomeArr, dayOfMonthX } = props.value\r\n  useEffect(() => {\r\n    let container = document.getElementById('b